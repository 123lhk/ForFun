@model WebInterface.Areas.UserManagement.Models.ExternalLoginListViewModel




	<div class="panel panel-danger">
		<div class="panel-heading">
			<h4>Use another service to log in</h4>
		</div>
		<div class="panel-body">
			@using (Html.BeginForm("ExternalLogin", "Account", new {ReturnUrl = Model.ReturnUrl}))
			{
				@Html.AntiForgeryToken()

				<button type="submit" class="btn btn-social btn-google-plus" id="GoogleLogin" name="provider" value="Google" title="Log in using your Google account">
					<i class="fa fa-google-plus"></i>
					Google
				</button>
			}
		</div>
	</div>    
       
    

